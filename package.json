  "repository": {
    "type": "git",
    "url": "https://github.com/ergenekonyigit/mwd-test"
  },
  "bugs": {
    "url": "https://github.com/ergenekonyigit/mwd-test/issues"
  },
  "homepage": "https://github.com/ergenekonyigit/mwd-test#readme",
  "dependencies": {
    "mathjs": "^7.5.1",
    "tslib": "~1.8.0"
  }
}
